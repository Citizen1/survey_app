 <header class="navbar navbar-fixed-top navbar-inverse">
  <div class="navbar-inner">
   <div class="container">
    <%= link_to "survey app", root_path, id: "logo" %>
    <nav>
     <ul class="nav pull-right">
      <li><%= link_to "Home",main_app.root_path %></li>
      <li><%= link_to "Help",main_app.help_path %></li>
      <% if signed_in? %>
							<li><%= link_to "Users", main_app.users_path %></li>
								<li><%= link_to "Survey", "/rapidfire" %></li>
							<li id="fat-menu" class="dropdown">
								<a href="#" class="dropdown-toggle" data-toggle="dropdown">
									Account <b class="caret"></b>
								</a>
								<ul class="dropdown-menu">
									<li><%= link_to "Profile", 	main_app.user_path(current_user) %></li>
									<li><%= link_to "Settings",
																	main_app.edit_user_path(current_user) %></li>
									<li class="divider"></li>
									<li>
										<%= link_to "Sign out", main_app.signout_path, method: "delete" %>
									</li>
								</ul>
							</li>
						<% else %>
							<li><%= link_to "Sign in", main_app.signin_path %></li>
						<% end %>
					</ul>
				</nav>
			</div>
		</div>
	</header>
